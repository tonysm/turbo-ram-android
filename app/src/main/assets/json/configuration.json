{
  "settings": {
    "screenshots_enabled": true
  },
  "rules": [
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "context": "default",
        "uri": "turbo://fragment/web",
        "fallback_uri": "turbo://fragment/web",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "/posts$"
      ],
      "properties": {
        "context": "default",
        "uri": "turbo://fragment/posts/index",
        "fallback_uri": "turbo://fragment/web",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "/posts/[0-9]+$"
      ],
      "properties": {
        "context": "default",
        "uri": "turbo://fragment/posts/show",
        "fallback_uri": "turbo://fragment/web",
        "pull_to_refresh_enabled": true
      }
    }
  ]
}